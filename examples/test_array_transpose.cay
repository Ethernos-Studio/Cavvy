// 大型测试：矩阵转置
public class TestArrayTranspose {
    public static void main() {
        int[][] matrix = new int[4][5]();
        int[][] transposed = new int[5][4]();
        
        // 初始化矩阵
        int value = 1;
        for (int i = 0; i < 4; i++) {
            for (int j = 0; j < 5; j++) {
                matrix[i][j] = value;
                value = value + 1;
            }
        }
        
        // 转置
        for (int i = 0; i < 4; i++) {
            for (int j = 0; j < 5; j++) {
                transposed[j][i] = matrix[i][j];
            }
        }
        
        // 验证
        boolean pass = true;
        for (int i = 0; i < 4; i++) {
            for (int j = 0; j < 5; j++) {
                if (matrix[i][j] != transposed[j][i]) {
                    pass = false;
                }
            }
        }
        
        if (pass) {
            println("Transpose test PASSED!");
        } else {
            println("Transpose test FAILED!");
        }
        
        // 打印原始矩阵
        println("Original matrix (4x5):");
        for (int i = 0; i < 4; i++) {
            for (int j = 0; j < 5; j++) {
                print(matrix[i][j]);
                print(" ");
            }
            println("");
        }
        
        // 打印转置矩阵
        println("Transposed matrix (5x4):");
        for (int i = 0; i < 5; i++) {
            for (int j = 0; j < 4; j++) {
                print(transposed[i][j]);
                print(" ");
            }
            println("");
        }
    }
}
